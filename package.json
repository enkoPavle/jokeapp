    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}' 'app/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .prettierignore",
    "pre-commit": "npm run format && npm run type-check && npm run lint && git add -A .",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.0.2",
